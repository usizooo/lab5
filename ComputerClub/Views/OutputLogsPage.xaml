<Page x:Class="ComputerClub.Views.OutputLogsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ComputerClub.Views"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="400"
      Title="OutputLogsPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <DataGrid x:Name="logsDataGrid" IsReadOnly="True" FontFamily="Cascadia Code SemiBold" Margin="5"/>
        <Button x:Name="backButton" Content="Назад" FontFamily="Cascadia Code SemiBold" Foreground="White" Grid.Row="3" Margin="5" Click="backButton_Click"/>
    </Grid>
</Page>
